{
    "name": "spatie/laravel-mailcoach-mailgun-feedback",
    "description": "Process feedback for email campaigns sent using Mailgun",
    "keywords": [
        "spatie",
        "laravel-mailcoach-mailgun-feedback"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
    {
        "type": "composer",
        "url": "https://satis.spatie.be"
    }
],
    "require": {
        "php": "^7.4",
        "illuminate/http": "^7.0|^8.0",
        "spatie/laravel-mailcoach": "^2.23.11",
        "spatie/laravel-webhook-client": "^2.7"
    },
    "require-dev": {
        "ext-json": "*",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachMailgunFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachMailgunFeedback\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachMailgunFeedback\\MailcoachMailgunFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
