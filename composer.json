{
    "name": "spatie/laravel-mailcoach-mailgun-feedback",
    "description": "Process feedback for email campaigns sent using Mailgun",
    "keywords": [
        "spatie",
        "laravel-mailcoach-mailgun-feedback"
    ],
    "homepage": "https://mailcoach.app",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "repositories": [
    {
        "type": "composer",
        "url": "https://satis.spatie.be"
    }
],
    "require": {
        "php": "^8.0",
        "illuminate/http": "^8.0",
        "spatie/laravel-mailcoach": "^4.0",
        "spatie/laravel-webhook-client": "^2.7"
    },
    "require-dev": {
        "ext-json": "*",
        "fakerphp/faker": "^1.12",
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4",
        "symfony/var-dumper": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MailcoachMailgunFeedback\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MailcoachMailgunFeedback\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MailcoachMailgunFeedback\\MailcoachMailgunFeedbackServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
